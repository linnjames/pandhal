<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
  <data noupdate="1">
    <record id="support_center_user_rule" model="ir.rule">
      <field name="name">Support Center User</field>
      <field name="model_id" ref="catalist_support.model_support_center"/>
      <field name="domain_force">['|',('assigned_user_id','=',user.id),('attended_user_id','=',user.id)]</field>
      <field name="groups" eval="[(4, ref('catalist_support.support_group_user'))]"/>
    </record>

    <record id="support_center_manager_rule" model="ir.rule">
      <field name="name">Support Center Manager</field>
      <field name="model_id" ref="catalist_support.model_support_center"/>
      <field name="domain_force">[(1,'=',1)]</field>
      <field name="groups" eval="[(4, ref('catalist_support.support_group_manager'))]"/>
    </record>
  </data>
</odoo>